{
    "name": "easyroadmap/easyroadmap",
    "description": "Ecommerce with WordPress made easy.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "autoload": {
        "psr-4": {
            "EasyRoadmap\\": "app/"
        },
        "files": ["app/Helper/functions.php", "app/Config/autoload.php"]
    },
    "authors": [
        {
            "name": "Codexpert, Inc"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "brain/monkey": "^2.6",
        "wp-cli/wp-cli": "^2.5",
        "squizlabs/php_codesniffer": "^3.7",
        "phpcompatibility/php-compatibility": "^9.3",
        "wp-coding-standards/wpcs": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}